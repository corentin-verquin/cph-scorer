  "license": "MIT",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "ts-standard --report stylish --fix src",
    "prettier": "prettier --write src",
    "format": "yarn lint && yarn prettier",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main/",
    "start:with-import": "NEED_IMPORT=true nest start",
    "test:e2e": "jest --runInBand --coverageDirectory .tmp_coverage/e2e test/e2e",
    "test:unit": "jest --coverageDirectory .tmp_coverage/unit --collectCoverageFrom=src/core/*.ts test/unit",
    "test:mutation": "stryker run",
    "pre-commit": "yarn format",
    "compile": "tsc -b --verbose"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4 ",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/swagger": "^5.0.8",
    "@nestjs/typeorm": "^8.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "jest-mock-extended": "^2.0.4",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.1",
    "@nestjs/testing": "^8.0.4",
    "@stryker-mutator/core": "^5.6.0",
    "@stryker-mutator/jest-runner": "^5.6.0",
    "@stryker-mutator/typescript-checker": "^5.6.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.1",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.7.0",
    "jest": "27.4.5",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "ts-standard": "^11.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  }
